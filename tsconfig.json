{
  "extends": "astro/tsconfigs/strictest",
    "exclude": ["dist"],
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@assets/*": ["src/assets/*"],
        "@components/*": ["src/components/*"],
        "@content/*": ["src/content/*"],
        "@images/*": ["src/assets/images/*"],
        "@layouts/*": ["src/layouts/*"],
        "@styles/*": ["src/styles/*"]
      },
      "jsx": "react-jsx"
    }
}
