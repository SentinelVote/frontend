---
import HEAD from '@components/HEAD.astro'
---
<!doctype html>
<html lang="en">
<HEAD title="Quantum Secure Vote | Login" description="Quantum Secure Vote" />
<body>
<form>
<label class="visually-hidden" for=username>NRIC</label>
<input id=username type=text placeholder=NRIC pattern="([Ss][1-9][0-9]|[Tt]0[0-9])[0-9]{5}[A-Za-z]" required>
<label class="visually-hidden" for=password>Password</label>
<input id=password type=password placeholder=Password required>
<button type="submit">Log in</button>
</form>
</body>
</html>